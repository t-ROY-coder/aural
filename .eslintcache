 {\r\n      p5.ellipse(Xcoord[i], Ycoord[i], 10);\r\n    }\r\n\r\n    Xcoord = [];\r\n    Ycoord = [];\r\n  };\r\n  return (\r\n    <>\r\n      <div className=\"container-fluid\">\r\n        <h2>Graph Analysis</h2>\r\n        <Sketch setup={setup} draw={draw} keyTyped={keyTyped} />\r\n      </div>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default AnalyzeGraph;\r\n",{"ruleId":"54","replacedBy":"55"},{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","severity":1,"message":"59","line":10,"column":11,"nodeType":"60","endLine":10,"endColumn":68},{"ruleId":"58","severity":1,"message":"59","line":13,"column":11,"nodeType":"60","endLine":16,"endColumn":12},{"ruleId":"61","severity":1,"message":"62","line":3,"column":13,"nodeType":"63","messageId":"64","endLine":3,"endColumn":17},{"ruleId":"61","severity":1,"message":"65","line":4,"column":10,"nodeType":"63","messageId":"64","endLine":4,"endColumn":21},{"ruleId":"61","severity":1,"message":"66","line":4,"column":23,"nodeType":"63","messageId":"64","endLine":4,"endColumn":32},{"ruleId":"61","severity":1,"message":"67","line":19,"column":5,"nodeType":"63","messageId":"64","endLine":19,"endColumn":6},{"ruleId":"61","severity":1,"message":"68","line":20,"column":5,"nodeType":"63","messageId":"64","endLine":20,"endColumn":6},"no-native-reassign",["69"],"no-negated-in-lhs",["70"],"react/jsx-no-target-blank","Using target=\"_blank\" without rel=\"noreferrer\" is a security risk: see https://html.spec.whatwg.org/multipage/links.html#link-type-noopener","JSXOpeningElement","no-unused-vars","'Tone' is defined but never used.","Identifier","unusedVar","'useLocation' is defined but never used.","'useParams' is defined but never used.","'x' is assigned a value but never used.","'y' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]